  "main": "index.js",
  "type": "module",
  "scripts": {
    "build:bubu": "gulp -f packages/components/script/build/index.ts",
    "build:hook": "pnpm run --filter @bubu-ui/hook build",
    "build:utils": "pnpm run --filter @bubu-ui/utils build",
