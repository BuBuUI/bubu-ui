    "docs:build": "pnpm run --filter site docs:build",
    "docs:deploy": "pnpm run --filter site docs:deploy",
    "test:bubu": "pnpm run --filter @bubu-ui/components test",
    "coverage:bubu": "pnpm run --filter @bubu-ui/components coverage",
    "release": "node scripts/release.mjs",
    "commit": "node scripts/commit.mjs",
    "dev": "pnpm run --filter ./play dev",
    "lint:script": "eslint --ext .js,.jsx,.vue,.ts,.tsx --fix --quiet ./",
    "lint:style": "stylelint --fix \"packages/components/src/**/*.{css,less}\"",
    "install:husky": "husky install",
    "changelog": "conventional-changelog -p  angular -i CHANGELOG.md -s",
    "prepare": "husky install"
  },
  "keywords": [],
